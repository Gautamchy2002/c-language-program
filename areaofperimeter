#include <stdio.h>
#include <math.h>

void main()
{
    
    void area();
    area();
}
    
void area()
{
    int a,b,c;
    float s,area;
    printf("enter the numbers");
    scanf("%d%d%d",&a,&b,&
    c);
    s=(a+b+c)/2;
    area=sqrt(s*(s-a)*(s-b)*(s-c));
    printf("the semiperimeter is %f",s);
    printf("the area is %f",area);
}
    
