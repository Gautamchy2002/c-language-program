#include <stdio.h>

int main()
{
   int i,n,a[i],s;
   printf("enter the size of the array");
   scanf("%d",&n);
   printf("enter the elements of an array");
   for(i=0;i<n;i++)
   scanf("%d",&a[i]);
   s=a[0];
   for(i=1;i<n; i++)
   {
   if(s<a[i])
   printf("the smallest number is %d",s);
   else
   s=a[i];
   
   }
   

    return 0;
}
